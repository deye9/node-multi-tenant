{
  "name": "node_multi_tenant",
  "version": "1.0.0",
  "description": "Multi-tenant Node.js Application",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "test-watch": "npm test -- --watch",
    "eslint": "eslint **/*.js --fix --quiet",
    "coverage": "npm test && open coverage/index.html",
    "test-integration": "node_modules/.bin/sequelize db:migrate --env=testing && nyc --reporter=html --reporter=text mocha ./tests/Integration --exit && open coverage/index.html",
    "test": "node_modules/.bin/sequelize db:migrate --env=testing && npm run eslint && nyc --reporter=html --reporter=text mocha tests --recursive --exit && open coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deye9/node_multi_tenant.git"
  },
  "keywords": [
    "Multi Tenant",
    "Multi Tenancy"
  ],
  "author": "Adegbenga A. Adeye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deye9/node_multi_tenant/issues"
  },
  "homepage": "https://github.com/deye9/node_multi_tenant#readme",
  "dependencies": {
    "auto-bind": "^2.0.0",
    "dotenv": "^6.2.0",
    "http-status-codes": "^1.3.0",
    "node-pg-migrate": "^3.15.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sequelize-cli": "^5.4.0",
    "sinon": "^7.2.3"
  }
}
