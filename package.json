{
  "name": "node-multi-tenant",
  "version": "1.0.0",
  "description": "Multi-tenant Node.js Application",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "node dist/index.js",
    "eslint": "eslint src/*.ts --fix --cache --quiet",
    "build": "rimraf dist && swc ./src -d dist && pnpm eslint",
    "test": "jest",
    "push:test": "jest --no-coverage --passWithNoTests --changedSince origin/main"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deye9/node-multi-tenant.git"
  },
  "keywords": [
    "Multi Tenant",
    "Multi Tenancy"
  ],
  "author": "Adegbenga A. Adeye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deye9/node-multi-tenant/issues"
  },
  "homepage": "https://github.com/deye9/node-multi-tenant#readme",
  "volta": {
    "node": "20.10.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@tsconfig/node20": "^20.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
